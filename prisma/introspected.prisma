model subscription {
  id                   String       @id
  plan                 String
  referenceId          String       @unique
  stripeCustomerId     String?
  stripeSubscriptionId String?
  status               String?
  periodStart          DateTime?
  periodEnd            DateTime?
  cancelAtPeriodEnd    Boolean?
  seats                Int?
  organization         Organization @relation(fields: [referenceId], references: [id], onDelete: Cascade)
}
